<% return if region.counties.all? { |county| moms_for(county).empty? } %>
<div class="mb-8 <%= class_names('no-free-capacity': !region.any_free_capacity?(@test_dates)) %>">
  <h2 class="text-2xl font-bold" id="<%= dom_id(region) %>"><%= region.name %></h2>
  <div class="mt-2">
    <%= render partial: 'county', collection: region.counties %>
  </div>
</div>
